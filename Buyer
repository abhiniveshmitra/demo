import java.util.List;

public class Buyer {
    private int buyerID;
    private String nickname;
    private int bitscoinBalance;
    private List<Bid> bids;
    private List<Message> messages;

    public Buyer(int buyerID, String nickname, int bitscoinBalance, List<Bid> bids, List<Message> messages) {
        this.buyerID = buyerID;
        this.nickname = nickname;
        this.bitscoinBalance = bitscoinBalance;
        this.bids = bids;
        this.messages = messages;
    }

    // Getter and setter methods for buyerID...
    public int getBuyerID() {
        return buyerID;
    }

    public void setBuyerID(int buyerID) {
        this.buyerID = buyerID;
    }

    // Getter and setter methods for nickname...
    public String getNickname() {
        return nickname;
    }

    public void setNickname(String nickname) {
        this.nickname = nickname;
    }

    // Getter and setter methods for bitscoinBalance...
    public int getBitscoinBalance() {
        return bitscoinBalance;
    }

    public void setBitscoinBalance(int bitscoinBalance) {
        this.bitscoinBalance = bitscoinBalance;
    }

    // Getter and setter methods for bids...
    public List<Bid> getBids() {
        return bids;
    }

    public void setBids(List<Bid> bids) {
        this.bids = bids;
    }

    // Getter and setter methods for messages...
    public List<Message> getMessages() {
        return messages;
    }

    public void setMessages(List<Message> messages) {
        this.messages = messages;
    }

    // Other buyer-specific methods...
}
