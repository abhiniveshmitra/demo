import java.util.Date;

public class Bid {
    private int bidID;
    private int bidAmount;
    private Buyer bidder;
    private Product product;
    private Date bidTime;
    private boolean isWinningBid;

    public Bid(int bidID, int bidAmount, Buyer bidder, Product product, Date bidTime, boolean isWinningBid) {
        this.bidID = bidID;
        this.bidAmount = bidAmount;
        this.bidder = bidder;
        this.product = product;
        this.bidTime = bidTime;
        this.isWinningBid = isWinningBid;
    }

    // Getter and setter methods for bidID...
    public int getBidID() {
        return bidID;
    }

    public void setBidID(int bidID) {
        this.bidID = bidID;
    }

    // Getter and setter methods for bidAmount...
    public int getBidAmount() {
        return bidAmount;
    }

    public void setBidAmount(int bidAmount) {
        this.bidAmount = bidAmount;
    }

    // Getter and setter methods for bidder...
    public Buyer getBidder() {
        return bidder;
    }

    public void setBidder(Buyer bidder) {
        this.bidder = bidder;
    }

    // Getter and setter methods for product...
    public Product getProduct() {
        return product;
    }

    public void setProduct(Product product) {
        this.product = product;
    }

    // Getter and setter methods for bidTime...
    public Date getBidTime() {
        return bidTime;
    }

    public void setBidTime(Date bidTime) {
        this.bidTime = bidTime;
    }

    // Getter and setter methods for isWinningBid...
    public boolean isWinningBid() {
        return isWinningBid;
    }

    public void setWinningBid(boolean winningBid) {
        isWinningBid = winningBid;
    }

    // Other bid-specific methods...
}
